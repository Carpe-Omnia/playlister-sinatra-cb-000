<h3> Here's the artist </h3>
<%= @artist.name %>
<ul>
  <% @artist.songs.each do |song| %>
    <li>
      Name:
      <a href= <%= "/songs/#{song.id}" %> >
        <%= song.name %>.
      </a>
      Genre:
      <a href= <%= "/genres/#{song.genre_id}" %> >
        <%= Genre.find_by(id: song.genre_id).name %>
      </a>
    </li>
  <% end %>
</ul>
